{
	"name": "floyd-cli",
	"version": "0.1.0",
	"license": "MIT",
	"bin": "dist/cli.js",
	"type": "module",
	"engines": {
		"node": ">=16"
	},
	"scripts": {
		"build": "tsc",
		"dev": "tsc --watch & sleep 2 && node dist/cli.js",
		"start": "node dist/cli.js",
		"lint": "prettier --check .",
		"test": "ava",
		"test:watch": "ava --watch",
		"test:components": "ava src/ui/components/__tests__",
		"test:modules": "ava src/__tests__/module-resolution.test.ts",
		"test:build": "ava src/__tests__/build-verification.test.ts",
		"test:runtime": "ava src/__tests__/runtime-verification.test.ts"
	},
	"files": [
		"dist"
	],
	"dependencies": {
		"@anthropic-ai/sdk": "^0.71.2",
		"@modelcontextprotocol/sdk": "^1.25.2",
		"@napi-rs/whisper": "^0.0.4",
		"@types/uuid": "^10.0.0",
		"@types/ws": "^8.18.1",
		"chokidar": "^3.6.0",
		"cli-chart": "^0.3.1",
		"diff": "^8.0.3",
		"dotenv": "^17.2.3",
		"electron": "40.0.0",
		"execa": "^9.5.2",
		"fast-glob": "^3.3.3",
		"floyd-agent-core": "file:../../packages/floyd-agent-core",
		"fs-extra": "^11.3.3",
		"fuzzysort": "^2.0.4",
		"gray-matter": "^4.0.3",
		"immer": "^10.1.1",
		"ink": "^4.1.0",
		"ink-autocomplete": "^0.0.0",
		"ink-big-text": "^2.0.0",
		"ink-chart": "^0.1.1",
		"ink-color-pipe": "^5.0.0",
		"ink-confirm-input": "^2.0.0",
		"ink-divider": "^4.1.0",
		"ink-figlet": "^1.0.3",
		"ink-form": "^2.0.1",
		"ink-gradient": "^3.0.0",
		"ink-image": "^2.0.0",
		"ink-link": "^4.0.0",
		"ink-markdown": "^1.0.4",
		"ink-multi-select": "^2.0.0",
		"ink-picture": "^1.3.3",
		"ink-progress-bar": "^3.0.0",
		"ink-scroll-view": "^0.3.5",
		"ink-scrollbar": "^1.0.0",
		"ink-select-input": "^5.0.0",
		"ink-spinner": "^5.0.0",
		"ink-syntax-highlight": "^2.0.2",
		"ink-tab": "^5.0.0",
		"ink-table": "^3.0.0",
		"ink-task-list": "^2.0.0",
		"ink-text-input": "^5.0.1",
		"keytar": "^7.9.0",
		"marked": "^12.0.2",
		"meow": "^11.0.0",
		"node-record-lpcm16": "^1.0.1",
		"node-tmux": "^1.0.2",
		"openai": "^6.16.0",
		"os-paths": "^7.4.0",
		"p-queue": "^7.4.1",
		"parse-diff": "^0.11.1",
		"puppeteer": "^23.11.1",
		"react": "^18.2.0",
		"simple-git": "^3.27.0",
		"uuid": "^13.0.0",
		"ws": "^8.19.0",
		"zod": "^4.3.5",
		"zustand": "^5.0.2"
	},
	"devDependencies": {
		"@sindresorhus/tsconfig": "^3.0.1",
		"@types/diff": "^5.0.9",
		"@types/fs-extra": "^11.0.4",
		"@types/node": "^22.10.5",
		"@types/react": "^18.0.32",
		"@vdemedes/prettier-config": "^2.0.1",
		"ava": "^5.2.0",
		"chalk": "^5.2.0",
		"eslint-config-xo-react": "^0.27.0",
		"eslint-plugin-react": "^7.32.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"ink-testing-library": "^3.0.0",
		"prettier": "^2.8.7",
		"ts-node": "^1.7.1",
		"tsx": "^4.21.0",
		"typescript": "^5.0.3",
		"xo": "^0.53.1"
	},
	"ava": {
		"extensions": {
			"ts": "module",
			"tsx": "module"
		},
		"nodeArguments": [
			"--import=tsx/esm"
		]
	},
	"xo": {
		"extends": "xo-react",
		"prettier": true,
		"rules": {
			"no-warning-comments": "off",
			"unicorn/expiring-todo-comments": "off",
			"unicorn/filename-case": "off",
			"unicorn/no-empty-file": "off",
			"import/order": "off",
			"import/extensions": "off",
			"import/no-cycle": "off",
			"capitalized-comments": "off",
			"react-hooks/exhaustive-deps": "off",
			"react/boolean-prop-naming": "off",
			"react/prefer-read-only-props": "off",
			"no-console": "off",
			"@typescript-eslint/no-unsafe-assignment": "off",
			"@typescript-eslint/no-unsafe-call": "off",
			"@typescript-eslint/no-unsafe-member-access": "off",
			"@typescript-eslint/no-unsafe-return": "off",
			"@typescript-eslint/no-unsafe-argument": "off",
			"@typescript-eslint/no-unsafe-constructor": "off",
			"@typescript-eslint/restrict-template-expressions": "off",
			"@typescript-eslint/restrict-plus-operands": "off",
			"@typescript-eslint/no-implicit-any-catch": "off",
			"@typescript-eslint/no-redundant-type-constituents": "off",
			"@typescript-eslint/ban-types": "off",
			"@typescript-eslint/no-floating-promises": "off",
			"@typescript-eslint/padding-line-between-statements": "off",
			"@typescript-eslint/no-loop-func": "off",
			"@typescript-eslint/consistent-type-imports": "off",
			"@typescript-eslint/prefer-readonly": "off",
			"@typescript-eslint/member-ordering": "off",
			"@typescript-eslint/naming-convention": "off",
			"@typescript-eslint/consistent-type-definitions": "off",
			"@typescript-eslint/no-base-to-string": "off",
			"@typescript-eslint/no-confusing-void-expression": "off",
			"@typescript-eslint/return-await": "off",
			"@typescript-eslint/no-extraneous-class": "off",
			"@typescript-eslint/prefer-nullish-coalescing": "off",
			"react/react-in-jsx-scope": "off",
			"unicorn/prevent-abbreviations": "off",
			"react/no-array-index-key": "off",
			"unicorn/prefer-spread": "off",
			"react/jsx-sort-props": "off",
			"n/prefer-global/process": "off",
			"unicorn/prefer-node-protocol": "off",
			"unicorn/no-static-only-class": "off",
			"unicorn/text-encoding-identifier-case": "off",
			"unicorn/catch-error-name": "off",
			"unicorn/prefer-optional-catch-binding": "off",
			"unicorn/no-lonely-if": "off",
			"no-return-await": "off",
			"no-await-in-loop": "off",
			"max-params": "off",
			"complexity": "off",
			"prefer-const": "off",
			"object-shorthand": "off",
			"@typescript-eslint/no-inferrable-types": "off",
			"unicorn/prefer-array-some": "off",
			"max-depth": "off",
			"prefer-destructuring": "off",
			"unicorn/numeric-separators-style": "off",
			"no-negated-condition": "off",
			"unicorn/switch-case-braces": "off",
			"@typescript-eslint/prefer-regexp-exec": "off",
			"unicorn/prefer-number-properties": "off",
			"unicorn/no-numeric-separators": "off"
		}
	},
	"prettier": "@vdemedes/prettier-config",
	"optionalDependencies": {
		"node-pty": "^1.1.0"
	}
}
