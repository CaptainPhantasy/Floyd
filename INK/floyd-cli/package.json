		"fs-extra": "^11.3.3",
		"ink": "^4.1.0",
		"ink-spinner": "^5.0.0",
		"ink-text-input": "^5.0.1",
		"meow": "^11.0.0",
		"openai": "^6.16.0",
		"os-paths": "^7.4.0",
		"react": "^18.2.0",
		"uuid": "^13.0.0",
		"ws": "^8.19.0",
		"zod": "^4.3.5"
	},
	"devDependencies": {
		"@sindresorhus/tsconfig": "^3.0.1",
		"@types/fs-extra": "^11.0.4",
